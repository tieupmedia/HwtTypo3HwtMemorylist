plugin.tx_hwtmemorylist {
    view {
        # cat=plugin.tx_hwtmemorylist/file; type=string; label=Path to template root (FE)
        templateRootPath = EXT:hwt_memorylist/Resources/Private/Templates/
		 # cat=plugin.tx_hwtmemorylist/file; type=string; label=Path to template partials (FE)
        partialRootPath = EXT:hwt_memorylist/Resources/Private/Partials/
		 # cat=plugin.tx_hwtmemorylist/file; type=string; label=Path to template layouts (FE)
        layoutRootPath = EXT:hwt_memorylist/Resources/Private/Layouts/
    }

    settings {
        javascript {
            jqueryUrl = https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js
            addJQueryFromGoogle = 1
            addJavaScript = 1
        }

        ajax {
            typeNum = 121212
            pid = 1
        }

		 # cat=plugin.tx_hwtmemorylist/file; type=string; label=Enable demo Templates (FE)
        enableDemoTemplates = 1
    }
}